{
  "name": "8tracks",
  "description": "8tracks for Desktop",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "1.0.0",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
      "storage",
      "webview",
      "http://8tracks.com/",
      "https://facebook.com/connect/*"
  ],
  "icons": {
    "16": "assets/icon-128x128.png",
    "128": "assets/icon-128x128.png"
  }
}
